{
  "name": "flaneur.io extension",
  "version": "1.0.0",
  "description": "Flaneur.io let's you search and explore your online-research",
  "browser_action": {
    "default_title": "Flaneur.io",
    "default_icon": "icon_128.png",
    "default_popup": "popup.html"
  },
  "background": {
    "page": "flaneur.html"
  },
  
 
  "permissions": [ "tabs", "http://*/*", "https://*/*", "file://*/*", "unlimitedStorage", 
  "*://use.typekit.net/*","webRequest", "webRequestBlocking"],
  "content_security_policy": "script-src 'self' https://use.typekit.net; object-src 'self'",
  "manifest_version": 2
}
