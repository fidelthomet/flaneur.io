{
  "name": "flaneur.io",
  "version": "1.1.0",
  "description": "Save and explore text fragments.",
  "browser_action": {
    "default_title": "Flaneur.io",
    "default_icon": {
      "19": "icons/flaneur.png",
      "38": "icons/flaneur38.png"
    },
    "default_popup": "popup.html"
  },
  "background": {
    "page": "flaneur.html"
  },
  "icons": { "16": "icons/flaneur16.png",
           "48": "icons/flaneur48.png",
          "128": "icons/flaneur128.png" 
  },
  
 
  "permissions": [ "clipboardWrite", "tabs", "http://*/*", "https://*/*", "unlimitedStorage", "*://use.typekit.net/*","webRequest", "webRequestBlocking"],
  "content_security_policy": "script-src 'self' https://use.typekit.net; object-src 'self'",
  "manifest_version": 2
}
